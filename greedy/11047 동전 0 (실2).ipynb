   "source": [
    "n,k = map(int, input().split())\n",
    "a = [int(input()) for _ in range(n)]\n",
    "\n",
    "a.reverse()\n",
    "\n",
    "count = 0\n",
    "for i in a:\n",
    "    if k // i >= 1:\n",
    "        count += k // i\n",
    "        k %= i\n",
    "print(count)\n"
   ]
  }
